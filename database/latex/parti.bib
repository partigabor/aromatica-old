@article{srinivasan_2007_black,
  title = {Black {{Pepper}} and Its {{Pungent Principle-Piperine}}: {{A Review}} of {{Diverse Physiological Effects}}},
  shorttitle = {Black {{Pepper}} and Its {{Pungent Principle-Piperine}}},
  author = {Srinivasan, K.},
