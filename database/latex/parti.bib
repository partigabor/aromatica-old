@article{croijmans_2020_uncovering,
  title = {Uncovering the {{Language}} of {{Wine Experts}}},
  author = {Croijmans, Ilja and Hendrickx, Iris and Lefever, Els and Majid, Asifa and Van Den Bosch, Antal},
